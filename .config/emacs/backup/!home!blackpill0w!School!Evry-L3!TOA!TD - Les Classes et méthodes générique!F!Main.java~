package F;
import java.util.*;

public class Main {
  public static void main(String args[]) {
    Couple<Integer, String> c = new Couple<Integer, String>(5, "Java");
    System.out.println(c);
    System.out.println(c.getVal());
    Integer[] tab = {7, 5, 21, 9, 0, 3, 4, 30};
    Couple.afficheTab(tab);
    Couple.swap(tab, 0, 2);
    Couple.afficheTab(tab);
    List<Integer> l = new ArrayList<Integer>();
    l.add(null);
    l.add(5);
    l.add(null);
    for (Integer i: tab)
      l.add(i);
    System.out.println("Max tab: " + Couple.max(l, 0, l.size()-1));
    System.out.println("Max tab: " + Couple.max(l, 1, 7));
    System.out.println("Max tab: " + Couple.max(l, -12, 856));
    System.out.println("Max tab: " + Couple.max(l, -12, 5));
    System.out.println("Max tab: " + Couple.max(l, 223, 988));
    System.out.println("Max tab: " + Couple.max(l, -7, -71));
    System.out.println("Max tab: " + Couple.max(l, 2, -71));
    System.out.println("Max tab: " + Couple.max(l, 2, 71));
    System.out.println("Max tab: " + Couple.max(l, 71, 1));

    System.out.println("n: " + Couple.elems_verifiant_propriete(l, ;
  }
}
